

<% if breakfast_time? %>
  <%= render 'static_pages/homepartials/breakfast_menu' %>
<% elsif lunch_time? %>
  <%= render 'static_pages/homepartials/lunch_menu' %>
<% elsif happy_hour_time? && dinner_time? %>
  <%= render 'static_pages/homepartials/happyhour_menu' %>
<% elsif dinner_time? %>
  <%= render 'static_pages/homepartials/dinner_menu' %>
<% elsif latenight_time? %>
  <%= render 'static_pages/homepartials/latenight_menu' %>    
<% else %>
  <% if brunch_time? %>
    <%= render 'static_pages/homepartials/brunch_menu' %>
  <% end %>
<% end %>

