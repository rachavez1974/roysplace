<% if breakfast_time? %>
  <%= render 'menus/partials_menu_for_customer/breakfast_menu' %>
<% elsif lunch_time? %>
  <%= render 'menus/partials_menu_for_customer/lunch_menu' %>
<% elsif happy_hour_time? && dinner_time? %>
  <%= render 'menus/partials_menu_for_customer/happyhour_menu' %>
<% elsif dinner_time? %>
  <%= render 'menus/partials_menu_for_customer/dinner_menu' %>
<% elsif latenight_time? %>
  <%= render 'menus/partials_menu_for_customer/latenight_menu' %>    
<% else %>
  <% if brunch_time? %>
    <%= render 'menus/partials_menu_for_customer/brunch_menu' %>
  <% end %>
<% end %>
